{
  "name": "secrets",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/backend/secrets/src",
  "projectType": "library",
  "tags": ["scope:backend", "type:lib"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/backend/secrets",
        "tsConfig": "libs/backend/secrets/tsconfig.lib.json",
        "packageJson": "libs/backend/secrets/package.json",
        "main": "libs/backend/secrets/src/index.ts",
        "assets": ["libs/backend/secrets/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/backend/secrets/jest.config.ts",
        "passWithNoTests": false
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/backend/secrets/**/*.ts"]
      }
    }
  }
}
