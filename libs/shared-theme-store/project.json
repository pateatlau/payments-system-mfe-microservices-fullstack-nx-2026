{
  "name": "shared-theme-store",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared-theme-store/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared-theme-store",
        "main": "libs/shared-theme-store/src/index.ts",
        "tsConfig": "libs/shared-theme-store/tsconfig.lib.json",
        "assets": ["libs/shared-theme-store/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared-theme-store/jest.config.js",
        "passWithNoTests": true
      }
    }
  }
}
