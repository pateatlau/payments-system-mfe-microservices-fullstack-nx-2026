{
  "name": "payments-system-mfe",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "nx": "./nx",
    "dev": "nx run-many --target=serve --projects=shell,hello-remote --parallel",
    "dev:shell": "nx serve shell",
    "dev:remote": "nx serve hello-remote",
    "build": "nx run-many --target=build --all",
    "build:shell": "nx build shell",
    "build:remote": "nx build hello-remote",
    "build:affected": "nx affected --target=build",
    "test": "nx run-many --target=test --projects=shell,hello-remote --parallel",
    "test:all": "nx run-many --target=test --all",
    "test:shell": "nx test shell",
    "test:remote": "nx test hello-remote",
    "test:affected": "nx affected --target=test",
    "test:ui": "nx test shell --ui",
    "lint": "nx run-many --target=lint --all",
    "lint:shell": "nx lint shell",
    "lint:remote": "nx lint hello-remote",
    "lint:affected": "nx affected --target=lint",
    "typecheck": "nx run-many --target=typecheck --all",
    "typecheck:shell": "nx typecheck shell",
    "typecheck:remote": "nx typecheck hello-remote",
    "typecheck:affected": "nx affected --target=typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "e2e": "nx run-many --target=e2e --all",
    "e2e:shell": "nx e2e shell-e2e",
    "e2e:remote": "nx e2e hello-remote-e2e",
    "graph": "nx graph",
    "reset": "nx reset"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@eslint/js": "^9.39.1",
    "@module-federation/enhanced": "0.21.6",
    "@module-federation/vite": "^1.9.2",
    "@nx/devkit": "22.1.3",
    "@nx/js": "22.1.3",
    "@nx/playwright": "22.1.3",
    "@nx/react": "^22.1.3",
    "@nx/vite": "^22.1.3",
    "@nx/vitest": "22.1.3",
    "@nx/web": "22.1.3",
    "@playwright/test": "^1.36.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "20.19.9",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^4.0.0",
    "@vitest/ui": "^4.0.0",
    "eslint": "^9.0.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "22.1.3",
    "prettier": "^3.3.0",
    "tslib": "^2.3.0",
    "typescript": "^5.9.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^7.0.0",
    "vite-plugin-dts": "~4.5.0",
    "vitest": "^4.0.0"
  },
  "dependencies": {
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router-dom": "6.29.0"
  }
}
