        "x-dead-letter-routing-key": "dead-letter"
      }
    },
    {
      "name": "payments.events.queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-dead-letter-exchange": "events.dlx",
        "x-dead-letter-routing-key": "dead-letter"
      }
    },
    {
      "name": "admin.events.queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-dead-letter-exchange": "events.dlx",
        "x-dead-letter-routing-key": "dead-letter"
      }
    },
    {
      "name": "profile.events.queue",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
